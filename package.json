{
  "name": "eslint-plugin-date-fns",
  "version": "0.2.0",
  "description": "date-fns specific linting rules for ESLint.",
  "type": "module",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "tsc && type-coverage && eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "npm run clean && npm run build && node --test --test-reporter=spec"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ChristianMurphy/eslint-plugin-date-fns.git"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "date",
    "date-fns"
  ],
  "author": "Christian Murphy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChristianMurphy/eslint-plugin-date-fns/issues"
  },
  "homepage": "https://github.com/ChristianMurphy/eslint-plugin-date-fns#readme",
  "peerDependencies": {
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@date-fns/tz": "^1.0.0",
    "@typescript-eslint/utils": "^8.0.0",
    "date-fns": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^24.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@typescript-eslint/rule-tester": "^8.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-eslint-plugin": "^7.0.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-sonarjs": "^3.0.0",
    "eslint-plugin-unicorn": "^61.0.0",
    "globals": "^16.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.0",
    "type-coverage": "^2.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0"
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "strict": true,
    "ignoreCatch": false
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
